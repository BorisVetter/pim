    "notLinkedWithProduct",
    "linkedWithCatalog",
    "onlyRootCategory",
    "onlyCatalogCategories",
    "onlyLeafCategories"
  ],
  "hiddenBoolFilterList": [
    "notEntity",
    "notLinkedWithProduct",
    "linkedWithCatalog",
    "onlyRootCategory",
    "onlyCatalogCategories",
    "onlyLeafCategories"
  ],
  "disabledMassActions": [
    "merge"
  ],
  "views": {
    "list": "pim:views/category/list",
